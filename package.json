{
  "name": "binder",
  "version": "2.0.0",
  "description": "Simple, yet powerful IoC container and service locator for both, the browser and node.",
  "keywords": [
    "ioc",
    "dependency-injection",
    "service-locator",
    "inversion-of-control"
  ],
  "main": "dist/binder.umd.js",
  "module": "dist/binder.es5.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Federico VÃ¡zquez <frondor.dev@gmail.com> (frondor.github.io)",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.js",
    "test": "jest --coverage",
    "preversion": "git merge origin/master && git push -u origin HEAD && npm ci && npm run lint && npm run build && npm test",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
